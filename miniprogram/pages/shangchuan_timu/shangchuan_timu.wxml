<!-- 上传题目页面 -->
<loading hidden="{{loadingHidden}}">
  数据加载中
</loading>
  <button class="upload" bindtap="up_timu" style="width:300rpx;height:100rpx;margin-left:20rpx;">
    <view class="jiahao" >+</view>
    上传题目
  </button>
<view wx:for="{{timu}}" class="container">
  <view class="notice">
          <view class="divLine" style="margin-right: 10rpx;"></view>你上传的文档<view class="divLine"  style="margin-left: 10rpx;"></view>
      </view>
  <view class="notice" style="margin-top:50rpx;" wx:if="{{my_upDoc.length===0}}">
         你还没有上传文档
   </view>
  <view class="item">
      <view class="timu">{{item.content}}</view>
      <view class="line">
        <image src="../../images/duoren.png" style="width:50rpx;height:50rpx;"></image>
        <view  data-id="{{item._id}}" class="fankui" bindtap="fankui">查看反馈</view>
      </view>
  </view>

  
</view>